num = int(input("Enter a number: "))
if num == 0:
print("Zero")
elif num > 0:
print("Positive")
else:
print("Negative")
marks = int(input("Enter marks: "))
if marks >= 75:
print("Distinction")
elif marks >= 60:
print("First Class")
elif marks >= 50:
print("Second Class")
elif marks >= 35:
print("Passed")
else:
print("Failed")
