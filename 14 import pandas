import pandas as pd
data = {
"Roll": [1, 2, 3, 4, 5],
"Name": ["A", "B", "C", "D", "E"],
"Attendance": [80, 90, 85, 75, 95]
}
df = pd.DataFrame(data, index=["RN1", "RN2", "RN3", "RN4", "RN5"])
print(df)
print(df.loc[["RN1", "RN3"]])
